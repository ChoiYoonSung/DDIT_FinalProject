<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>


<iframe class="content-wrapper"name="ifr" src="main.do" frameborder="0" style="background-color:#fff; border-radius:15px; width:100%;height:80vh;"></iframe>
 	 


<!-- handlebars -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.7.6/handlebars.min.js"></script>

<script type="text/x-handlebars-template"  id="subMenu-list-template" >

{{#each .}}
	<div class="menu-item {{#ifEq murl 'thirdSubMenu'}}has-sub{{/ifEq}} subMenu">
		<a href="javascript:{{#ifEq murl 'thirdSubMenu'}} thirdSubMenu('{{mcode}}');{{else}} goPage('{{murl}}','{{mcode}}');isActive('{{mcode}}');initPageParam(); {{/ifEq}}" class="menu-link">
			<div class="menu-icon">{{micon}}	</div>
			<div class="menu-text">{{mname}}</div>
			{{#ifEq murl 'thirdSubMenu'}}<div class="menu-caret"></div>{{/ifEq}}
			
		</a>
			{{#ifEq murl 'thirdSubMenu'}}
				<div class="menu-submenu thirdSubMenuList">
				</div>
			{{/ifEq}}
	</div>
{{/each}}
</script>
<script type="text/x-handlebars-template" id="thirdSubMenu-list-template">
{{#each .}}
	{{#ifEq mlevel 3}}
		<!-- 2depth -->
			<div class="menu-item thirdSubMenu">
				<a href="javascript:goPage('{{murl}}','{{mcode}}');isActive('{{mcode}}');initPageParam();" class="menu-link"><div class="menu-text">{{mname}}</div></a>
			</div>
	{{/ifEq}}
{{/each}}
</script>
<script>
Handlebars.registerHelper('ifEq', function(v1, v2, options) {
	  if(v1 == v2) {
	    return options.fn(this);
	  }
	  return options.inverse(this);
	});
</script>
<script>
function init(){
	goPage('${menu.murl}','${menu.mcode}');
	subMenu('${menu.mcode}'.substring(0,3)+"0000");
	thirdSubMenu('${menu.mcode}'.substring(0,5)+"00");
};

</script>








