package com.spring.dto;

public class PAAttachVO {
	private String paCode; // 자료실 번호
	private String paAtPath; // 올리는 경로
	private String paAtName; // 파일 이름
	private String paAtType; // 파일 확장자
	private int paAtNo; // 파일 번호
	
	public String getPaCode() {
		return paCode;
	}
	public void setPaCode(String paCode) {
		this.paCode = paCode;
	}
	public String getPaAtPath() {
		return paAtPath;
	}
	public void setPaAtPath(String paAtPath) {
		this.paAtPath = paAtPath;
	}
	public String getPaAtName() {
		return paAtName;
	}
	public void setPaAtName(String paAtName) {
		this.paAtName = paAtName;
	}
	public String getPaAtType() {
		return paAtType;
	}
	public void setPaAtType(String paAtType) {
		this.paAtType = paAtType;
	}
	public int getPaAtNo() {
		return paAtNo;
	}
	public void setPaAtNo(int paAtNo) {
		this.paAtNo = paAtNo;
	}
	
	
	
}
