package com.spring.dto;

public class SmailVO {

	private String smCode       ;
	private String empId        ;
	private String smTitle      ;
	private String smContent    ;
	private String smReceiver   ;
	private int smRstatus    ;
	private String smSenddate   ;
	private String smReceivedate;
	private int smEnabled    ;
	
	
	
	public String getSmCode() {
		return smCode;
	}
	public void setSmCode(String smCode) {
		this.smCode = smCode;
	}
	public String getEmpId() {
		return empId;
	}
	public void setEmpId(String empId) {
		this.empId = empId;
	}
	public String getSmTitle() {
		return smTitle;
	}
	public void setSmTitle(String smTitle) {
		this.smTitle = smTitle;
	}
	public String getSmContent() {
		return smContent;
	}
	public void setSmContent(String smContent) {
		this.smContent = smContent;
	}
	public String getSmReceiver() {
		return smReceiver;
	}
	public void setSmReceiver(String smReceiver) {
		this.smReceiver = smReceiver;
	}
	public int getSmRstatus() {
		return smRstatus;
	}
	public void setSmRstatus(int smRstatus) {
		this.smRstatus = smRstatus;
	}
	public String getSmSenddate() {
		return smSenddate;
	}
	public void setSmSenddate(String smSenddate) {
		this.smSenddate = smSenddate;
	}
	public String getSmReceivedate() {
		return smReceivedate;
	}
	public void setSmReceivedate(String smReceivedate) {
		this.smReceivedate = smReceivedate;
	}
	public int getSmEnabled() {
		return smEnabled;
	}
	public void setSmEnabled(int smEnabled) {
		this.smEnabled = smEnabled;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
