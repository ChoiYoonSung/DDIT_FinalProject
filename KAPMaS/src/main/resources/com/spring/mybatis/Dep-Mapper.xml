<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="Dep-Mapper">



<select id="getDepList"  resultType="dep">

select
*
from DEPARTMENT
</select>



<select id="getDepListByCode" parameterType="str" resultType="dep">


select
*
from DEPARTMENT
where DEP_CODE=#{depCode}

</select>











</mapper>
















